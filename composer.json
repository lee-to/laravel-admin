{
  "name": "lee-to/laravel-admin",
  "description": "Laravel admin",
  "keywords": ["laravel", "admin"],
  "type": "library",
  "homepage": "https://github.com/lee-to/laravel-admin",
  "license": "MIT",
  "authors": [
    {
      "name": "Danil Shutsky",
      "email": "leetodev@ya.ru",
      "homepage": "https://github.com/lee-to"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "phpoffice/phpspreadsheet": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.35 || ^5.7",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Leeto\\Admin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Leeto\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Leeto\\Admin\\Providers\\AdminServiceProvider"
      ]
    }
  }
}
